@using OnlineShop.Models
@using System.Configuration
@{
    var db = new OnlineShopEntities();

}
<header>
    <div class="header-container">
        <div class="header-area header-sticky pt-30 pb-30">
            <div class="container">
                <div class="row ltr">
                    <!--Header Logo Start-->
                    <div class="col-lg-3 col-md-3">
                        <div class="logo-area">
                            <a href="/">
                                <img src="@(ConfigurationManager.AppSettings["LOGO_1"])" alt="" class="royanLogo">
                            </a>
                        </div>
                    </div>
                    <!--Header Logo End-->
                    <!--Header Menu And Mini Cart Start-->
                    <div class="col-lg-9 col-md-9 text-lg-right ltr">
                        <!--Main Menu Area Start-->
                        <div class="header-Menu ltr">
                            <nav>
                                <ul class="main-Menu rtl">



                                    @{
                                        var Menus = db.Menus.Where(r => r.Parent == null).OrderByDescending(r => r.priority).ToList();
                                        if (Menus.Count != 0)
                                        {
                                            int i = 0;
                                            foreach (var item in Menus)
                                            {
                                                if (i == 1)
                                                {

                                                    <li class="showInLaptop">
                                                        <a href="#">
                                                            محصولات
                                                        </a>
                                                        <!--Dropdown Menu Start-->
                                                        <ul class="dropdown">
                                                            @{
                                                                var mainCats = db.MainCats.OrderBy(r => r.priority).ToList();
                                                                if (mainCats.Count != 0)
                                                                {
                                                                    foreach (var item2 in mainCats)
                                                                    {
                                                                        string tl = item2.title;
                                                                        while (tl.Contains(" "))
                                                                        {
                                                                            tl = tl.Replace(" ", "_");
                                                                        }
                                                                        <li class="listMenuParent">
                                                                            <a href="/home/mainCat/@item2.id/@tl">@item2.title</a>
                                                                            @{
                                                                                var subCats = db.Categories.Where(r => r.MainCat == item2.id).ToList();
                                                                                if (subCats.Count != 0)
                                                                                {
                                                                                    <ul class="listMenu hidden">
                                                                                        @foreach (var item22 in subCats)
                                                                                        {
                                                                                            string tlt = item22.title;
                                                                                            while (tlt.Contains(" "))
                                                                                            {
                                                                                                tlt = tlt.Replace(" ", "_");
                                                                                            }
                                                                                            <li>
                                                                                                <a href="/home/category/@item22.id/@tlt">
                                                                                                    @item22.title
                                                                                                </a>
                                                                                            </li>


                                                                                        }
                                                                                    </ul>
                                                                                }
                                                                            }
                                                                        </li>
                                                                                    }
                                                                                }
                                                            }

                                                        </ul>
                                                        <!--Dropdown Menu End-->
                                                    </li>
                                                    <li class="showInLaptop">
                                                        <a href="#">
                                                            وبلاگ
                                                        </a>
                                                        <!--Dropdown Menu Start-->
                                                        <ul class="dropdown">
                                                            @{
                                                                var blogCats = db.Blog_Cats.OrderBy(r => r.id).ToList();
                                                                if (blogCats.Count != 0)
                                                                {
                                                                    foreach (var item2 in blogCats)
                                                                    {
                                                                        string tl = item2.title;
                                                                        while (tl.Contains(" "))
                                                                        {
                                                                            tl = tl.Replace(" ", "_");
                                                                        }
                                                                        <li class="listMenuParent">
                                                                            <a href="/blog/category/@item2.id/@tl">@item2.title</a>
                                                                            @{
                                                                                var subCats = db.Blog_post.Where(r => r.catId == item2.id).ToList();
                                                                                if (subCats.Count != 0)
                                                                                {
                                                                                    <ul class="listMenu hidden">
                                                                                        @foreach (var item22 in subCats)
                                                                                        {
                                                                                            string tlt = item22.title;
                                                                                            while (tlt.Contains(" "))
                                                                                            {
                                                                                                tlt = tlt.Replace(" ", "_");
                                                                                            }
                                                                                            <li>
                                                                                                <a href="/blog/details/@item22.id/@tlt">
                                                                                                    @item22.title
                                                                                                </a>
                                                                                            </li>


                                                                                        }
                                                                                    </ul>
                                                                                }
                                                                            }
                                                                        </li>
                                                                                    }
                                                                                }
                                                            }

                                                        </ul>
                                                        <!--Dropdown Menu End-->
                                                    </li>

                                                    <li class="showinMobile">
                                                        <a href="#">محصولات</a>
                                                        <!--Mega Menu Start-->
                                                        <ul class="mega-Menu">
                                                            @{
                                                                var MainCats = db.MainCats.ToList();
                                                                MainCats = MainCats.OrderBy(r => r.priority).ToList();
                                                                if (MainCats.Count != 0)
                                                                {
                                                                    foreach (var item2 in MainCats)
                                                                    {
                                                                        string tl = item2.title;
                                                                        while (tl.Contains(" "))
                                                                        {
                                                                            tl = tl.Replace(" ", "_");
                                                                        }
                                                                        <li>
                                                                            <a href="/home/mainCat/@item2.id/@tl" class="item2-link">@item2.title</a>
                                                                            <ul>
                                                                                @{
                                                                                    var subCats = db.Categories.Where(r => r.MainCat == item2.id).ToList();
                                                                                    if (subCats.Count != 0)
                                                                                    {
                                                                                        foreach (var item22 in subCats)
                                                                                        {
                                                                                            string tlt = item22.title;
                                                                                            while (tlt.Contains(" "))
                                                                                            {
                                                                                                tlt = tlt.Replace(" ", "_");
                                                                                            }
                                                                                            <li><a href="/home/category/@item22.id/@tlt">@item22.title</a></li>

                                                                                        }
                                                                                    }

                                                                                }

                                                                            </ul>
                                                                        </li>

                                                                                        }
                                                                                    }
                                                            }
                                                        </ul>
                                                        <!--Mega Menu End-->
                                                    </li>
                                                    <li class="showinMobile">
                                                        <a href="#">وبلاگ</a>
                                                        <!--Mega Menu Start-->
                                                        <ul class="mega-Menu">
                                                            @{
                                                                if (blogCats.Count != 0)
                                                                {
                                                                    foreach (var item2 in blogCats)
                                                                    {
                                                                        string tl = item2.title;
                                                                        while (tl.Contains(" "))
                                                                        {
                                                                            tl = tl.Replace(" ", "_");
                                                                        }
                                                                        <li>
                                                                            <a href="/blog/category/@item2.id/@tl" class="item2-link">@item2.title</a>
                                                                            <ul>
                                                                                @{
                                                                                    var subCats = db.Blog_post.Where(r => r.catId == item2.id).ToList();
                                                                                    if (subCats.Count != 0)
                                                                                    {
                                                                                        foreach (var item22 in subCats)
                                                                                        {
                                                                                            string tlt = item22.title;
                                                                                            while (tlt.Contains(" "))
                                                                                            {
                                                                                                tlt = tlt.Replace(" ", "_");
                                                                                            }
                                                                                            <li><a href="/blog/details/@item22.id/@tlt">@item22.title</a></li>

                                                                                        }
                                                                                    }

                                                                                }

                                                                            </ul>
                                                                        </li>

                                                                                        }
                                                                                    }
                                                            }
                                                        </ul>
                                                        <!--Mega Menu End-->
                                                    </li>
                                                                                    }
                                                                                    i += 1;
                                                                                    if (db.Menus.Where(r => r.Parent == item.id).FirstOrDefault() == null)
                                                                                    {
                                                                                        <li><a href="@item.link">@item.title</a></li>


                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <li>
                                                                                            <a href="@item.link">@item.title</a>
                                                                                            <!--Dropdown Menu Start-->
                                                                                            <ul class="dropdown">
                                                                                                @{
                                                                                                    var subMenus = db.Menus.Where(r => r.Parent == item.id).ToList();
                                                                                                    if (subMenus.Count != 0)
                                                                                                    {
                                                                                                        foreach (var item2 in subMenus)
                                                                                                        {
                                                                                                            <li><a href="@item2.link">@item2.title</a></li>

                                                                                                        }
                                                                                                    }
                                                                                                }
                                                                                            </ul>
                                                                                            <!--Dropdown Menu End-->
                                                                                        </li>
                                                                                                            }

                                                                                                        }
                                                                                                    }
                                    }

                                    @{
                                        if (Session["userLogin"] == null)
                                        {
                                            <li>
                                                <a href="#">
                                                    <img src="~/Theme/img/user.svg" style="width:30px;display:inline-block" />
                                                    ورود / عضویت
                                                </a>
                                                <!--Dropdown Menu Start-->
                                                <ul class="dropdown">
                                                    <li><a href="/home/login">ورود</a></li>
                                                    <li><a href="/home/register">عضویت</a></li>
                                                </ul>
                                                <!--Dropdown Menu End-->
                                            </li>
                                        }
                                    }

                                </ul>
                            </nav>
                        </div>
                        <!--Main Menu Area End-->
                        <!--Header Option Start-->
                        <div class="header-option">
                            <div class="mini-cart-search ltr">
                                @{
                                    var ordersList = Session["orders"] as List<Order>;
                                    if (ordersList != null && ordersList.Count != 0)
                                    {
                                        <div class="mini-cart">
                                            <a href="#">
                                                <span class="cart-icon">
                                                    <span class="cart-quantity">@ordersList.Count</span>
                                                </span>
                                                <span class="cart-title">سبد خرید <br></span>
                                            </a>
                                            <!--Cart Dropdown Start-->
                                            <div class="cart-dropdown">
                                                <ul>
                                                    @{
                                                        foreach (var item in ordersList)
                                                        {
                                                            var product = db.Products.Where(r => r.id == item.productId).FirstOrDefault();
                                                            var gallery = db.ProductGalleries.Where(r => r.product_id == product.id).FirstOrDefault();
                                                            string tlt = product.title;
                                                            var discount = db.Discounts.Where(r => r.productId == item.productId && r.status == 1 && r.percentage != null).FirstOrDefault();
                                                            Int64 realPrice = Convert.ToInt64(product.price);
                                                            if (item.SellerId == null)
                                                            {
                                                                if (discount != null)
                                                                {
                                                                    realPrice = Convert.ToInt64(product.price - ((product.price * discount.percentage) / 100));
                                                                }

                                                            }
                                                            else
                                                            {
                                                                var sellerProduct = db.SellersProducts.Where(r => r.sellerId == item.SellerId && r.productId == product.id).FirstOrDefault();
                                                                if (sellerProduct != null)
                                                                {
                                                                    realPrice = Convert.ToInt64(sellerProduct.price);
                                                                }
                                                            }
                                                            while (tlt.Contains(" "))
                                                            {
                                                                tlt = tlt.Replace(" ", "_");
                                                            }

                                                            <li class="single-cart-item">
                                                                <div class="cart-img">
                                                                    <a href="@tlt"><img src="@System.Configuration.ConfigurationManager.AppSettings["uploadwebpath"]@gallery.big" alt=""></a>
                                                                </div>
                                                                <div class="cart-content">

                                                                    <h5 class="product-name"><a href="/home/product/@product.id/@tlt">@product.title</a></h5>
                                                                    <span class="cart-price">@item.quantity × @realPrice</span>
                                                                </div>
                                                                <div class="cart-remove">
                                                                    <a title="Remove" href="#"><i class="fa fa-times"></i></a>
                                                                </div>
                                                            </li>


                                                        }
                                                    }

                                                </ul>
                                                <p class="cart-subtotal">
                                                    <strong class="float-right">جمع کل:</strong>
                                                    @{
                                                        Int64 sum = 0;
                                                        foreach (var item2 in ordersList)
                                                        {
                                                            var pr = db.Products.Where(r => r.id == item2.productId).FirstOrDefault();
                                                            var discount = db.Discounts.Where(r => r.productId == pr.id && r.status == 1 && r.percentage != null).FirstOrDefault();
                                                            Int64 realPrice = 0;
                                                            if (item2.SellerId == null)
                                                            {
                                                                realPrice = Convert.ToInt64(pr.price);
                                                                if (discount != null)
                                                                {
                                                                    realPrice = Convert.ToInt64(pr.price - ((pr.price * discount.percentage) / 100));
                                                                }
                                                                sum += Convert.ToInt64(realPrice * item2.quantity);
                                                            }
                                                            else
                                                            {
                                                                var sellerProduct = db.SellersProducts.Where(r => r.sellerId == item2.SellerId && r.productId == pr.id).FirstOrDefault();
                                                                sum += Convert.ToInt64(sellerProduct.price * item2.quantity);
                                                            }


                                                        }
                                                    }
                                                    <span class="float-left">@sum تومان</span>
                                                    <div class="clearfix"></div>
                                                </p>
                                                <p class="cart-btn">
                                                    <a href="/home/card">سبد خرید</a>
                                                    <a href="/home/invoice">پیش فاکتور</a>
                                                </p>
                                            </div>
                                            <!--Cart Dropdown End-->
                                        </div>

                                                        }
                                }

